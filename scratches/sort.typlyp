delo partition(arr: chislo<>, left: chislo, right: chislo, pivot: chislo) -> chislo {
    poka (left <= right) {
        poka (arr[left] < pivot) {
            left + 1 -> left;
        }

        poka (arr[right] > pivot) {
            right <- right - 1;
        }

        esli (left <= right) {
            temp: chislo <- arr[left];
            arr[left] <- arr[right];
            temp -> arr[right];

            left <- left + 1;
            right - 1 -> right;
        }
    }

    otday <- left;
}

delo quickSort(arr: chislo<>, left: chislo, right: chislo) -> chislo {
    esli (left < right) {
        pivot: chislo <- arr[(left + right) / 2];
        index: chislo <- partition <- [arr, left, right, pivot];

        quickSort <- [arr, left, index - 1];
        quickSort <- [arr, index, right];
    }

    otday <- 0;
}

