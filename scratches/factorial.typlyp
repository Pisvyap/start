vneshnaya narisovatChislo(x: chislo): chislo;

delo factorial(n: chislo): chislo {
    pust k: chislo = 0;
    esli (n == 0) {
        k = 1;
    } inache {
        k = n * factorial(n - 1);
    }

    vernut k;
}

pust number: chislo = 20;
pust result: chislo = factorial(number);
narisovatChislo(result);