delo print(arr: logika<>): chislo {
    arr[0] = pravda;
    arr[0] -> glaza;
    vernut 0;
}
pust primes: logika<> = new logika<100>;
primes[0] = lozh;
primes[1] = lozh;
schitaem (pust p: chislo = 0; p < 100; p = p + 1;) {
    primes[p] = pravda;
    primes[p] -> glaza;
}
pust n: chislo = 0;
poka (primes[10] != lozh) {
    primes[n] = lozh;
    primes[n] -> glaza;
    n = n + 1;
}
pust b: chislo = print(primes);