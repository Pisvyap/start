pust numbers: chislo<> = new chislo<10000>;
pust temp: chislo = 0;

schitaem (pust i: chislo = 0 ; i < 10000; i = i + 1;) {
    numbers[i] = i;
}

schitaem (pust j: chislo = 0 ; j < 10000; j = j + 1;){
    schitaem (pust z: chislo = 0 ; z < 10000; z = z + 6;){
        temp = numbers[j];
        numbers[j] = numbers[z];
        numbers[z] = temp;
    }
}

schitaem (pust k: chislo = 100 ; k < 120; k = k + 1;){
    numbers[k] -> glaza;
}



