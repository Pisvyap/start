delo quickSort(arr: chislo[], left: chislo, right: chislo): chislo[] {
    esli (left >= right) {
        vernut arr;
    }

    pust pivot: chislo = arr[(left + right) / 2];
    pust index: chislo = partition(arr, left, right, pivot);

    arr = quickSort(arr, left, index - 1);
    arr = quickSort(arr, index, right);

    vernut arr;
}